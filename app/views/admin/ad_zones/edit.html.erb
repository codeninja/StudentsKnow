<h1>Editing ad_zone</h1>

<%= error_messages_for :ad_zone %>

<% form_for(@ad_zone, :url => admin_ad_page_ad_zone_path(:ad_page_id => @adpage.id)) do |@f| %>
    <%= render :partial => "form" %>
    <%= @f.submit "Update" %>
    <%= link_to("Cancel", admin_ad_page_ad_zones_path(:ad_page_id => @adpage.id)) %>
<% end %>

<%= link_to 'Back', admin_ad_page_path(:ad_page_id => @adpage.id) %>
